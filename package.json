{
  "name": "ldbc-snb-validation-generator.js",
  "version": "1.1.0",
  "packageManager": "yarn@1.22.22",
  "description": "Generates SPARQL validation queries and results",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/SolidBench/ldbc-snb-validation-generator.js.js#readme",
  "repository": "git@github.com:SolidBench/ldbc-snb-validation-generator.js.js.git",
  "bugs": {
    "url": "https://github.com/SolidBench/ldbc-snb-validation-generator.js.js/issues"
  },
  "keywords": [
    "ldbc",
    "snb",
    "benchmarking",
    "validation"
  ],
  "main": "lib/index.js",
  "typings": "lib/index",
  "bin": {
    "ldbc-snb-validation-generator.js": "./bin/runner.js"
  },
  "lsd:module": true,
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "lint": "eslint . --cache",
    "build": "yarn run build:ts && yarn run build:components",
    "build:components": "componentsjs-generator -s lib -i .componentsignore",
    "build:ts": "tsc",
    "validate": "yarn list",
    "prepare": "husky && yarn run build",
    "version": "manual-git-changelog onversion"
  },
  "dependencies": {
    "@rdfjs/types": "*",
    "@types/fs-extra": "^11.0.0",
    "componentsjs": "^6.0.0",
    "fs-extra": "^11.0.0",
    "sparql-query-parameter-instantiator": "^2.6.0"
  },
  "devDependencies": {
    "@rubensworks/eslint-config": "^3.1.0",
    "@types/jest": "^29.0.0",
    "arrayify-stream": "^2.0.0",
    "componentsjs-generator": "^4.0.0",
    "eslint": "^8.0.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "jest-rdf": "^1.8.0",
    "manual-git-changelog": "^1.0.0",
    "rdf-data-factory": "^1.1.0",
    "stream-to-string": "^1.2.0",
    "streamify-array": "^1.0.1",
    "streamify-string": "^1.0.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "resolutions": {
    "@types/node": "^22.0.0"
  }
}
